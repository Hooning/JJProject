                                <h2 class="section-heading"><%=title%></h2>
                                <h3 class="section-subheading text-muted"><%=desc%></h3>
                            </div>
                        </div>
                        <div class="row">
                    <%
                    for( var j = 0; j < nav[i].list.length; j++ ){                                    
                        var musicHeader = nav[i].list[j].header;
                        var musicDesc = nav[i].list[j].description;
                        var musicImg = nav[i].list[j].img;
                        var details = nav[i].list[j].detail;
                        var modalLink = "#musicModal" + (j+1);
                        var modal = "musicModal" + (j+1);
                            
                    %>
                        <div class="col-md-4 col-sm-6 portfolio-item">
                            <a href="<%=modalLink %>" class="portfolio-link" data-toggle="modal">
                                <div class="portfolio-hover">
                                    <div class="portfolio-hover-content">
                                        <i class="fa fa-plus fa-3x"></i>
                                    </div>
                                </div>
                                <img src="<%=musicImg %>" class="img-responsive" alt="">
                            </a>
                            <div class="portfolio-caption">
                                <h4><%=musicHeader %></h4>
                                <p class="text-muted"><%=musicDesc %></p>
                            </div>
                        </div>

                        <!-- Music Modals -->
                        <div class="portfolio-modal modal fade" id="<%=modal %>" tabindex="-1" role="dialog" aria-hidden="true">
                            <div class="modal-dialog">
                                <div class="modal-content">
                                    <div class="close-modal" data-dismiss="modal">
                                        <div class="lr">
                                            <div class="rl">
                                            </div>
                                        </div>
                                    </div>
                                    <div class="container">
                                        <div class="row">
                                            <div class="col-lg-8 col-lg-offset-2">
                                                <div class="modal-body">
                                                    <!-- Project Details Go Here -->
                                                    <h2>
                                                        <%=musicHeader %>
                                                    </h2>
                                                    <p class="item-intro text-muted">
                                                        <%=musicDesc %>
                                                    </p>
                                                    <img class="img-responsive img-centered" src="<%=musicImg %>" alt="">
                                                    <ul class="list-inline">
                                                        <% 
                                                for( var k = 0; k < details.length; k++){
                                                    var bgYear = details[k].begin_year;
                                                    var edYear = details[k].end_year;
                                                    var detail = details[k].detail; 
                                                    var year = '';
                                                    if ( bgYear === edYear ){
                                                        year = '[ '+bgYear + ' ]';    
                                                    }else{
                                                        year = '[ '+bgYear + ' - ' + edYear + ' ]';
                                                    }
                                                %>
                                                    <li>
                                                      <%=year %>
                                                      <%=detail %>
                                                    </li>
                                                    </BR>
                                                <% 
                                                }
                                                %>
                                                    </ul>
                                                    <button type="button" class="btn btn-primary" data-dismiss="modal"><i class="fa fa-times"></i> Close Project</button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    <%            
                    }
                    %>
                        </div>
                    </div>
                </section>
        <%
        }
        %>

        <%
        if( menu == "Gallery" ){
            console.log("## Gallery : " + menu + " - Loaded");
            var title = nav[i].title;
            var desc = nav[i].description;       
        %>
                <!-- Gallery Section -->
                <section id="gallery" class="bg-light-gray">
                    <div class="container">
                        <div class="row">
