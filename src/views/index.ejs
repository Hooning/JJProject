    <!-- Header -->
    <header>
        <div class="container">
            <div class="intro-text">
                <div class="intro-lead-in">
                    <%=welcome %>
                </div>
                <div class="intro-heading">
                    <%=whatAbout %>
                </div>
            </div>
        </div>
    </header>

    <% for( var i = 0; i < nav.length; i++){
        
        var menu = nav[i].menu;
        if( menu == "About" ){
            console.log("## Menu : " + menu + " - Loaded");
            var title = nav[i].title;
            var desc = nav[i].description;
            %>
        
        <!-- Introduction Section -->
        <section id="about">
            <div class="container">
                <div class="row">
                    <div class="col-lg-12 text-center">
                        <h2 class="section-heading">
                            <%=title%>
                        </h2>
                        <h3 class="section-subheading text-muted">
                            <%=desc%>
                        </h3>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-12">
                        <ul class="timeline">
                            <% 
                                for( var j = 0; j < nav[i].list.length; j++ ){                                    
                                    var imgUrl = nav[i].list[j].img;
                                    var bgDate = nav[i].list[j].begin_date;
                                    var bgYear = bgDate.substring(0,4);
                                    var bgMonth = bgDate.substring(5,7);
                                    var edDate = nav[i].list[j].end_date;
                                    var edYear = edDate.substring(0,4);
                                    var edMonth = edDate.substring(5,7);
                                    
                                    if( edDate == "9999-12-31"){
                                        edDate = "";
                                        edYear = "";
                                        edMonth = "";
                                    }
                                    
                                    var month = ["Jan","Feb","Mar","Apr","May","Jun","Jul","Aug","Sep","Oct","Nov","Dec"];
                                    
                                    bgMonth = month[parseInt(bgMonth)-1];
                                    edMonth = month[parseInt(edMonth)-1];
                                    
                                    var head = nav[i].list[j].header;
                                    if( j % 2 == 0){     
                            %>
                                <li>
                                    <div class="timeline-image">
                                        <img class="img-circle img-responsive" src="<%=imgUrl %>" alt="">
                                    </div>
                                    <div class="timeline-panel">
                                        <div class="timeline-heading">
                                            <h4>
                                                <%=bgMonth %>
                                                    <%=bgYear %> ~
                                                        <% if ( edDate != "" ){ %>
                                                            <%=edMonth %>
                                                                <%=edYear%>
                                                                    <% } %>
                                            </h4>
                                            <h4 class="subheading">
                                                <%=head %>
                                            </h4>
                                        </div>
                                        <div class="timeline-body">
                                            <% for( var k = 0; k < nav[i].list[j].detail.length; k++){ %>
                                                <p class="text-muted">
                                                    <%=nav[i].list[j].detail[k]%>
                                                </p>
                                                <% } %>
                                        </div>
                                    </div>
                                </li>
                                <%}else{ %>
                                    <li class="timeline-inverted">
                                        <div class="timeline-image">
                                            <img class="img-circle img-responsive" src="<%=imgUrl %>" alt="">
                                        </div>
                                        <div class="timeline-panel">
                                            <div class="timeline-heading">
                                                <h4>
                                                    <%=bgMonth %>
                                                        <%=bgYear %> ~
                                                            <% if ( edDate != "" ){ %>
                                                                <%=edMonth %>
                                                                    <%=edYear%>
                                                                        <% } %>
                                                </h4>
                                                <h4 class="subheading">
                                                    <%=head %>
                                                </h4>
                                            </div>
                                            <div class="timeline-body">
                                                <% for( var k = 0; k < nav[i].list[j].detail.length; k++){ %>
                                                    <p class="text-muted">
                                                        <%=nav[i].list[j].detail[k]%>
                                                    </p>
                                                    <% } %>
                                            </div>
                                        </div>
                                    </li>
                                    <% } %>
                                        <% } %>
                                            <li class="timeline-inverted">
                                                <div class="timeline-image">
                                                    <div class="text-centered">
                                                        <h4>Moved<br/>to<br/>Germany</h4>
                                                    </div>
                                                </div>
                                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </section>
        <% } 
         %>


        <!-- Music Grid Section -->
        <%
        if( menu == "Music" ){
