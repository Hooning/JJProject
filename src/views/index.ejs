                        <a href="#page-top"></a>
                    </li>
                    <% for(var i=0; i<nav.length; i++){%>
                        <li>
                            <a class="page-scroll" href="<%=nav[i].link%>">
                                <%=nav[i].menu%>
                            </a>
                        </li>
                        <%}%>
                </ul>
